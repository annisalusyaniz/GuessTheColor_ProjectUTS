@import url('https://fonts.googleapis.com/css2?family=Lato:wght@100&family=Signika+Negative&display=swap');
body {
    background-color: #F6E7D8;
    font-family: 'Signika Negative', sans-serif;
    font-style: normal;
    font-size: 45px;
    line-height: 55px;
    color: #000000;
}
.btn{
	width:120px;
	height:70px;
	box-sizing: border-box;
    background: #C65D7B;
	border-radius: 10px;
    border:4px solid #874356;
	position: absolute;
	top: 20px;
	left: 91%;
	padding-top: 10px;
	padding-left: 10px;
}
#countdown {
	font-size: 40px;
    line-height: 55px;
}
#container {
    margin-top: 40xpx;
	margin-left: 36%;
	margin-bottom: 40px;
    max-width: 500px;
}
.round {
    width: 20%;
    background: purple;
    padding-bottom: 20%;
    float: left;
    margin-left: 30px;
    margin-top: 30px;
    border-radius: 100%;
    transition: background 0.6s;
    -webkit-transition: background 0.6s;
    -moz-transition: background 0.6s;
}
h1 {
    text-align: center;
    line-height: 1.1;
    font-weight: normal;
    margin: 0;
    text-transform: uppercase;
}
h2 {
    margin-top: 10px;
    text-align: center;
    font-size: 35px;
}